begin declare input : natural,
              output : natural,
              repnr : natural,
              rep : natural;
      input := 14;
      repnr := input + (1 + 2);
      output := 1;
      while input - 1 do
          rep := output;
          repnr := input;
          while repnr - "1" do
             output := output + rep + "1";
             repnr := repnr - 1
          od;
          input := input - 1
      od;
      if input - "1" then
        rep := output
      else
        output := rep
      fi
end